{
  "logo_threshold": 0.65,
  "allowed_boards": ["cbse", "cisce", "maharashtra", "tamil nadu", "karnataka"],
  "patterns": {
    "name": "(?i)name.*?(?:\\:|\\s{2,})([A-Z][A-Z .]+)",
    "board": "(?i)(board|university|institution).*?(\\w+(?:\\s+\\w+)*)",
    "year": "(?i)(year|passing|dated).*?(\\d{4})",
    "percentage": "(?i)(percentage|marks?\\s*obtained).*?(\\d{2,3}(?:\\.\\d{1,2})?)",
    "roll": "(?i)(roll|seat|enrolment).{0,5}(\\d{6,12})"
  }
}
